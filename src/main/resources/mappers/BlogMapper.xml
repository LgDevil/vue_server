<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.auxing.vue_server.mapper.BlogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.auxing.vue_server.entity.Blog">
        <id column="ID" property="id" />
        <result column="TITLE" property="title" />
        <result column="HTML" property="html" />
        <result column="STARTDATE" property="startdate" />
        <result column="AUTHOR" property="author" />
        <result column="BLOGINDEX" property="blogindex" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, TITLE, HTML, STARTDATE, AUTHOR,BLOGINDEX
    </sql>

</mapper>
